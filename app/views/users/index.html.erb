<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Role</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.user_role %></td>
        <td>
          <%= link_to 'View', user_path(user) %>
          <%= link_to 'Edit', edit_user_path(user) if can? :update, user %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>